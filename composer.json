{
  "name": "w2w/apie-object-access-normalizer",
  "description": "Symfony normalizers used internally by Apie",
  "require": {
    "php": "^7.2",
    "phpdocumentor/reflection-docblock": "^4.3",
    "symfony/http-kernel": "^4.3|^5.0",
    "symfony/property-info": "^4.3|^5.0",
    "symfony/serializer": "^4.3|^5.0",
    "psr/cache": "^1.0"
  },
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "W2w\\Lib\\ApieObjectAccessNormalizer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "W2w\\Test\\ApieObjectAccessNormalizer\\": "tests/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.3",
    "phpunit/phpunit": "^8.5",
    "ramsey/uuid": "^3.8",
    "w2w/apie": "^3.2"
  }
}
